    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "20.0.4",
    "prettier": "^1.16.4",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.24.1",
    "bootstrap": "3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
