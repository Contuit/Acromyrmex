    "font-awesome": "4.7.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.2",
    "moment": "2.17.1",
    "normalize-scss": "6.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "prop-types": "~15.5.10",
    "rc-calendar": "^9.6.0",
    "rc-time-picker": "^3.3.1",
